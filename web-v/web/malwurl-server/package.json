{
  "name": "malwurl-server",
  "version": "0.0.1",
  "private": true,
  "author": "Anthony Verez <netantho@gmail.com>",
  "description": "Awesome web interface for the malwURL project, machine learning for malicious URLs",
  "scripts": {
