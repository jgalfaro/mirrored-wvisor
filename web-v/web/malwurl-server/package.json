    "lodash": "*",
    "caterpillar": "~2.0.2",
    "caterpillar-filter": "~2.0.1",
    "caterpillar-human": "~2.1.0",
    "async": "*",
    "sqlite3": "~2.1.19",
    "grunt-contrib-watch": "~0.5.3",
    "tar": "~0.1.19",
    "fstream": "~0.1.25",
    "mime": "~1.2.11",
    "cookie-signature": "~1.0.1",
    "bytes": "~0.2.1",
    "batch": "~0.5.0",
    "negotiator": "~0.3.0",
    "tail": "~0.3.5"
  },
  "devDependencies": {
    "grunt": "~0.4.2",
    "grunt-contrib-jshint": "~0.6.3",
    "grunt-contrib-nodeunit": "~0.2.0"
